{
  "name": "jjk-ui",
  "displayName": "JJK UI",
  "version": "1.0.3",
  "packageManager": "pnpm@8.6.7",
  "description": "A Vue 3 UI library for jojko.tech and other personal projects",
  "author": {
    "name": "Jacob Janisz",
    "email": "jacob@jojko.tech"
  },
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/JoJk0"
  },
  "homepage": "https://ui.jojko.tech",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoJk0/jjk-ui.git"
  },
  "bugs": {
    "url": "https://github.com/JoJk0/jjk-ui/issues"
  },
  "keywords": [
    "vue",
    "vue3",
    "ui",
    "library",
    "component",
    "components",
    "jjk",
    "jojko",
    "jojk0",
    "jojko.tech"
  ],
  "maintainers": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "default": "./dist/vite.mjs"
    },
    "./styles": "./dist/style.css",
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist"
      ]
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "histoire dev",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "build": "vue-tsc --noEmit && vite build",
    "stories:build": "histoire build",
    "stories:test": "percy exec pnpm run build --",
    "release": "semantic-release",
    "preview": "histoire preview",
    "prepare": "husky install",
    "commitlint": "commitlint --edit"
  },
  "peerDependencies": {
    "vue": "^3.3.2"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": false
    }
  },
  "readme": "./README.md",
  "sponsor": {
    "url": "https://github.com/sponsors/JoJk0"
  },
  "contributors": [
    {
      "name": "Jacob Janisz",
      "email": "jacob@jojko.tech"
    }
  ],
  "badges": [
    {
      "title": "npm",
      "description": "npm package",
      "url": "https://www.npmjs.com/package/jjk-ui",
      "href": "https://www.npmjs.com/package/jjk-ui",
      "image": "https://img.shields.io/npm/v/jjk-ui?color=blue&logo=npm&style=flat-square"
    },
    {
      "title": "GitHub",
      "description": "source code",
      "url": "https://github.com/JoJk0/jjk-ui",
      "href": "https://github.com/JoJk0/jjk-ui"
    }
  ],
  "dependencies": {
    "@oruga-ui/oruga-next": "^0.6.0",
    "@vueuse/core": "^10.2.1",
    "v-wave": "^1.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.7",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@histoire/plugin-percy": "^0.16.1",
    "@histoire/plugin-vue": "^0.16.1",
    "@iconify-json/carbon": "^1.1.18",
    "@oruga-ui/oruga-next": "^0.6.0",
    "@percy/cli": "^1.26.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^20.4.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue-macros/reactivity-transform": "^0.3.14",
    "@vue-macros/volar": "^0.12.1",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.14",
    "commitlint": "^17.6.6",
    "eslint": "8.45.0",
    "histoire": "^0.16.2",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "pnpm": "^8.6.7",
    "postcss": "^8.4.26",
    "sass": "^1.63.6",
    "semantic-release": "^21.0.7",
    "simple-git-hooks": "^2.8.1",
    "tailwindcss": "^3.3.3",
    "taze": "^0.11.2",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "unplugin-vue-macros": "^2.3.6",
    "vite": "^4.4.4",
    "vite-plugin-dts": "^3.3.0",
    "vitest": "^0.33.0",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.vue": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu"
    ]
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "channel": "next"
      }
    ],
    "repositoryUrl": "https://github.com/JoJk0/jjk-ui.git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "dist/**/*.{js,css}",
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/changelog"
    ]
  }
}
