{
  "name": "jjk-ui",
  "displayName": "JJK UI",
  "version": "1.0.4",
  "packageManager": "pnpm@10.2.1",
  "description": "A Vue UI library for jojko.tech and other personal projects",
  "author": {
    "name": "Jacob Janisz",
    "email": "jacob@jojko.tech"
  },
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/JoJk0"
  },
  "homepage": "https://ui.jojko.tech",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoJk0/jjk-ui.git"
  },
  "bugs": {
    "url": "https://github.com/JoJk0/jjk-ui/issues"
  },
  "keywords": [
    "vue",
    "vue3",
    "ui",
    "library",
    "component",
    "components",
    "jjk",
    "jojko",
    "jojk0",
    "jojko.tech"
  ],
  "maintainers": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "default": "./dist/vite.mjs"
    },
    "./styles": "./dist/jjk-ui.css",
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist"
      ]
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "histoire dev",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "build": "vue-tsc --noEmit && vite build",
    "stories:build": "histoire build",
    "stories:test": "percy exec pnpm run build --",
    "release": "semantic-release",
    "preview": "histoire preview",
    "prepare": "husky install",
    "commitlint": "commitlint --edit"
  },
  "peerDependencies": {
    "vue": "^3.3.2"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": false
    }
  },
  "readme": "./README.md",
  "sponsor": {
    "url": "https://github.com/sponsors/JoJk0"
  },
  "contributors": [
    {
      "name": "Jacob Janisz",
      "email": "jacob@jojko.tech"
    }
  ],
  "badges": [
    {
      "title": "npm",
      "description": "npm package",
      "url": "https://www.npmjs.com/package/jjk-ui",
      "href": "https://www.npmjs.com/package/jjk-ui",
      "image": "https://img.shields.io/npm/v/jjk-ui?color=blue&logo=npm&style=flat-square"
    },
    {
      "title": "GitHub",
      "description": "source code",
      "url": "https://github.com/JoJk0/jjk-ui",
      "href": "https://github.com/JoJk0/jjk-ui"
    }
  ],
  "dependencies": {
    "@iconify/vue": "^4.3.0",
    "@oruga-ui/oruga-next": "^0.9.3",
    "@oruga-ui/theme-oruga": "^0.5.1",
    "@vueuse/core": "^12.5.0",
    "normalize.css": "^8.0.1",
    "radix-vue": "^1.9.13",
    "v-wave": "^3.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@histoire/plugin-percy": "^0.17.17",
    "@histoire/plugin-vue": "^1.0.0-alpha.1",
    "@iconify-json/carbon": "^1.2.6",
    "@oruga-ui/oruga-next": "^0.9.3",
    "@percy/cli": "^1.30.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^20.17.17",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue-macros/reactivity-transform": "^1.1.6",
    "@vue-macros/volar": "^0.30.14",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "commitlint": "^19.7.1",
    "eslint": "9.20.0",
    "eslint-plugin-format": "^1.0.1",
    "histoire": "^1.0.0-alpha.1",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "pnpm": "^10.2.1",
    "postcss": "^8.5.1",
    "sass": "^1.84.0",
    "semantic-release": "^24.2.1",
    "simple-git-hooks": "^2.11.1",
    "taze": "^18.4.0",
    "typescript": "^5.7.3",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "unplugin-vue-macros": "^2.14.2",
    "vite": "^6.1.0",
    "vite-css-modules": "^1.8.4",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-static-copy": "^2.2.0",
    "vitest": "^3.0.5",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.vue": "eslint --fix"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "channel": "next"
      }
    ],
    "repositoryUrl": "https://github.com/JoJk0/jjk-ui.git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "dist/**/*.{js,css}",
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/changelog"
    ]
  }
}
